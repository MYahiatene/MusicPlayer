<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="630.0"
            xmlns="http://javafx.com/javafx/11.0.1">
  <children>
    <Label layoutX="14.0" layoutY="20.0" text="Current Song:"/>
