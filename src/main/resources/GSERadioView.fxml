            text="Start Player"/>
    <Label layoutX="14.0" layoutY="45.0" text="Artist:"/>
    <Label layoutX="14.0" layoutY="68.0" text="Title:"/>
    <Label layoutX="14.0" layoutY="91.0" text="Duration:"/>
    <Label layoutX="14.0" layoutY="118.0" text="Album:"/>
    <Label layoutX="14.0" layoutY="146.0" text="Genre:"/>
    <Label fx:id="label1" layoutX="105.0" layoutY="20.0" text=""/>
    <Label fx:id="label2" layoutX="54.0" layoutY="45.0" text=""/>
    <Label fx:id="label3" layoutX="54.0" layoutY="68.0" text=""/>
    <Label fx:id="label4" layoutX="75.0" layoutY="91.0" text=""/>
    <Label fx:id="label5" layoutX="61.0" layoutY="118.0" text=""/>
    <Label fx:id="label6" layoutX="57.0" layoutY="146.0" text=""/>
    <TableView fx:id="tableView" layoutX="337.0" prefHeight="400.0" prefWidth="654.0">
        <columns>
          <TableColumn fx:id="column1" prefWidth="238.0" text="Artist"/>
          <TableColumn fx:id="column2" prefWidth="282.0" text="Title"/>
          <TableColumn fx:id="column3" prefWidth="132.0" text="Votes"/>
        </columns>
      </TableView>
    <ComboBox layoutX="120.0" layoutY="259.0" prefWidth="150.0" fx:id="dropDown"/>
    <Label layoutX="57.0" layoutY="264.0" prefHeight="16.0" prefWidth="61.0" text="Songs"/>
    <Button layoutX="270.0" layoutY="259.0" mnemonicParsing="false" text="vote" fx:id="voteButton"
            onAction="#countVote"/>
  </children>
</AnchorPane>
